{
  "compilerOptions": {
    "strict": true,
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true
  }
}
